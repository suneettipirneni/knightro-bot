{
  "name": "knightro-bot",
  "version": "1.0.0",
  "description": "A discord bot to fulfill the needs of UCF students.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development && nodemon --exec ts-node --files ./src/index.ts",
    "start:prod": "export NODE_ENV=production && tsc && node ./dist/index.js",
    "test": "jest ./src/__tests__",
    "lint": "eslint --fix ./src",
    "format": "prettier --write ./src",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suneettipirneni/knightro-bot.git"
  },
  "author": "Suneet Tipirneni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suneettipirneni/knightro-bot/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/suneettipirneni/knightro-bot#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/humps": "^2.0.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@knighthacks/dispatch": "^0.1.0-dev-21d7e85",
    "axios": "^0.21.1",
    "discord.js": "^13.1.0",
    "discord.js-embed-pagination": "^0.5.3",
    "dotenv": "^10.0.0",
    "humps": "^2.0.1"
  }
}
